{% extends 'base.html.twig' %}

{% block title %}Hello CoachAgendaController!
{% endblock %}

{% block body %}

    {{ form_start(form) }}
    <div class="form-group">
        {{ form_row(form.title) }}
    </div>
    <div class="form-group">
        {{ form_row(form.description) }}
    </div>
    <div class="form-group">
        {{ form_row(form.durationActual) }}
    </div>
    <div class="form-group">
        {{ form_row(form.coach) }}
    </div>
    <div class="form-group">
        {{ form_label(form.startTime, 'Start: ') }}<br>
        {{ form_widget(form.startTime) }}
    </div>
    <div class="form-group">
        {{ form_row(form.commentClient) }}
    </div>

    {{ form_row(form.save) }}

    {{ form_end(form) }}

{% endblock %}